{
  "name": "https-callable-enforcer",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/https-callable-enforcer/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/https-callable-enforcer",
        "main": "packages/https-callable-enforcer/src/index.ts",
        "tsConfig": "packages/https-callable-enforcer/tsconfig.lib.json",
        "assets": ["packages/https-callable-enforcer/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/https-callable-enforcer/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/https-callable-enforcer/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": ["slice:lib"]
}
